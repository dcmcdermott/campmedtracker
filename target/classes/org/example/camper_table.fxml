<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ControllerCamperTable">
   <children>
      <VBox alignment="CENTER" prefHeight="322.0" prefWidth="424.0" spacing="20.0">
            <Label text="Campers" />
            <TableView fx:id="tvCampers" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="col_id" prefWidth="75.0" text="Camper ID" />
                <TableColumn fx:id="col_last_name" prefWidth="75.0" text="Last Name" />
                  <TableColumn fx:id="col_first_name" prefWidth="75.0" text="First Name" />
                  <TableColumn fx:id="col_contact" prefWidth="75.0" text="Contact" />
                  <TableColumn fx:id="col_comments" prefWidth="75.0" text="Comments" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         <Button fx:id="btnSwitchToPrescriptions" mnemonicParsing="false" onAction="#switchToPrescriptions" text="Switch to Prescriptions" />
          <Button fx:id="btnSwitchToAddCamper" mnemonicParsing="false" onAction="#switchToAddCamper" text="Add Camper" />
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
