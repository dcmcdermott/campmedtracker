<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.ControllerPrescriptionTable">
    <children>
        <VBox alignment="CENTER" prefHeight="322.0" prefWidth="424.0" spacing="20.0">
            <Label text="Prescriptions" />
            <TableView fx:id="tvCampers" prefHeight="200.0" prefWidth="200.0">
                <columns>
                    <TableColumn fx:id="col_id" prefWidth="75.0" text="Prescriptions ID" />
                    <TableColumn fx:id="col_med_name" prefWidth="75.0" text="Medication" />
                    <TableColumn fx:id="col_dose" prefWidth="75.0" text="Dose" />
                    <TableColumn fx:id="col_admin_time" prefWidth="75.0" text="Administration Time" />
                    <TableColumn fx:id="col_camper_id" prefWidth="75.0" text="Camper ID" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
            <Button fx:id="btnSwitchToCampers" mnemonicParsing="false" onAction="#switchToCampers" text="Switch to Campers" />
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>